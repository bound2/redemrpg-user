{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "*": [
                "types/*"
            ]
        },
        "outDir": "./",
        "noImplicitAny": false,
        "module": "es6",
        "target": "es2017",
        "allowJs": true,
        "lib": [
            "es2017"
        ],
        "types": [
            "redm-client-cfx",
            "redm-natives",
            "@types/node"
        ],
        "moduleResolution": "node",
        "experimentalDecorators": true
    },
    "include": [
        "./**/*"
    ],
    "exclude": []
}